  def my_iterator(data):
    for item in data:
        yield item 
        
for i in my_iterator([10,20,30]):
    print(i)


a = [10,20,30]

c = iter(a)
print(next(c))
print(next(c))


def my_generator():
    yield 1
    yield 2
    yield 3

gen = my_generator()

print(next(gen))  # 1
print(next(gen))  # 2
print(next(gen))  # 3
# next(gen) ‚Üí StopIteration


itertor implementation manually 
-------------------------------
class Counter:
    
    def __init__(self,low, high):
        self.low = low 
        self.high = high 
        
    def __iter__(self):
        return self 
        
    def __next__(self):
        if self.low > self.high:
            raise StopIteration
        num = self.low 
        self.low+=1 
        return num 

for i in Counter(1,3):
    print(i)



