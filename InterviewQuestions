import asyncio

async def fetch_data():
    print("Start fetching")
    await asyncio.sleep(2)  # Simulates a network delay
    print("Done fetching")
    return {'data': 123}

async def main():
    print("Start main")
    data = await fetch_data()
    print(f"Received data: {data}")

asyncio.run(main())

